<% include partials/header_facebook%>
 <div class="page-wrap">

      <nav class="navbar navbar-default">
        <div class="container">
          <div class="navbar-header">
            <a class="navbar-brand" href="/campgrounds">Sebonache</a>
          </div>
          <!--TODO Fix collapse-->
          <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav navbar-right">
                <li><a href="/facebook2">Facebook</a></li>
                <% if(!currentUser){ %>
                    <li><a href="/login">Login</a></li>
                    <li><a href="/register">Sign-up</a></li>
               <% }else{ %>
                <li><a href="#">Signed in as <%= currentUser.username %></a></li>
                <li><a href="/logout">Logout</a></li>
               <% } %> 
               <li><a id="logout" href="#" onclick="logout()">Facebook Logout</a></li>
            <fb:login-button
              id="fb-btn"
              scope="public_profile,email,user_birthday,user_location,user_posts,user_friends,user_status,user_photos,user_likes"
              onlogin="checkLoginState();">
            </fb:login-button>
            </ul>
          </div>
        </div>
      </nav>
</div> 
    

    <div class="container">
      <h3 id="heading">Log in to view your profile</h3>
      <div id="profile"></div>
      <div id="cover"></div>      
    </div>


